{
  "name": "miniurl",
  "version": "1.0.0",
  "description": "minimize url",
  "main": "index.js",
  "scripts": {
    "balancer": "nodemon loadbalancer/app.js",
    "url": "nodemon app/app.js",
    "start": "MINIURL_PORT=3000 node app/app.js",
    "dev": "MINIURL_PORT=3000 nodemon app/app.js",
    "test": "exit 1",
    "ls": "autocannon http://localhost:3000/list -d 5",
    "go": "autocannon http://localhost:3000/z -d 1",
    "save": "autocannon http://localhost:3000/save -m POST -H content-type=application/json -b '{\"url\":\"www.google.ca\"}' -d 10"
  },
